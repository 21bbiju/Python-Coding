#creatin tuples with differnt data types
tuplex = ("tuple", False, 3.2, 1)
print(tuplex)
#creating a tuple
tuplex = (4, 6, 2, 8, 3, 1)
print(tuplex)
#tuples are unchangeable cannot add any new elements unless you add two tuplets together
tuplex = tuplex + (9,)
print(tuplex)
#counts how may 50s are in the tuplet
tuple1 = (50, 10, 60, 70, 50)
print(tuple1.count(50))

tuplex = (2, 4, 3, 5, 4, 6, 7, 8, 6, 1)
_slice = tuplex[3:5]
print(_slice)
_slice = tuplex[:6]
print(_slice)