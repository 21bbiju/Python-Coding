try:
    num1, num2 = eval(input("Enter two numbers, seprated by a coma : "))
    result = num1 / num2
    print("result is in") , result
except ZeroDivisionError:
    print("Division by a zero is error")
except SyntaxError:
    print("coma is missing enter numbers seprated by a coma like this 1, 2")
except:
    print("wrong input")
else:
    print("no exceptions")
finally:
    print("this will execute no matter what")
